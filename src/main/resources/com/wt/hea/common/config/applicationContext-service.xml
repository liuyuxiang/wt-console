<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="
	http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
	http://www.springframework.org/schema/context
	http://www.springframework.org/schema/context/spring-context-3.0.xsd">
	<bean id="heaBaseService" class="com.wt.hea.common.service.impl.BaseService">
		<property name="behaveLogDao" ref="_BehaveLogDao"/>
		<property name="systemCodeDao" ref="heaSystemCodeDao"/>
	</bean> 

	<bean id="_BehaveLogService" class="com.wt.hea.common.infrastructure.logger.impl.LoggerService" parent="heaBaseService"/>
	<bean id="heaSystemCodeService" class="com.wt.hea.common.service.impl.SystemCodeServiceImpl"  parent="heaBaseService"/>
</beans>